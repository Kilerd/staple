article = {SOI ~ meta ~ content ~ EOI}

meta = { "\n"* ~ (inlineMeta ~ "\n"+)+ }

inlineMeta = { " "* ~ "-" ~ " "* ~ inlineKey ~  " "* ~ "=" ~ " "* ~ inlineValue }

inlineKey = { (!("\n" | " " | "=") ~ ANY)+ }
inlineValue = { (!("\n") ~ ANY)+ }
content = { ANY* }